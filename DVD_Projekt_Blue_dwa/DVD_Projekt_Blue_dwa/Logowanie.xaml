<Window x:Class="DVD_Projekt_Blue_dwa.Logowanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DVD_Projekt_Blue_dwa"
        mc:Ignorable="d"
        Title="Logowanie" Height="170" Width="492">
    <Grid Height="145">
        <Label Content="Login:" HorizontalAlignment="Left" Margin="55,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Hasło:" HorizontalAlignment="Left" Margin="59,72,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="login" HorizontalAlignment="Left" Margin="126,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="zaloguj" Content="zaloguj" HorizontalAlignment="Left" Margin="321,36,0,0" VerticalAlignment="Top" Click="zaloguj_Click"/>
        <Label Content="(login: alfa)" HorizontalAlignment="Left" Margin="321,63,0,0" VerticalAlignment="Top"/>
        <Label Content="(hasło: dystrybuanta)" HorizontalAlignment="Left" Margin="325,94,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="hasełko" HorizontalAlignment="Left" Margin="126,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
